{
  "name": "defom-ai-backend",
  "version": "1.0.0",
  "description": "Backend API Server for DefomAI",
  "main": "api.py",
  "scripts": {
    "start": "gunicorn api:app --workers 4 --worker-class uvicorn.workers.UvicornWorker --bind 0.0.0.0:$PORT --timeout 600",
    "dev": "uvicorn api:app --host 0.0.0.0 --port 8000 --reload"
  },
  "engines": {
    "node": "18.x"
  }
}
